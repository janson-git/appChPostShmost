{
  "manifest_version": 2,
  "name": "Poster-Shmoster",
  "version": "0.1",
  "minimum_chrome_version": "51",
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["js/lib/jquery-2.2.4.min.js", "public/background.js"]
    }
  },
  "permissions": [
    "http://*/",
    "https://*/*",
    "storage"
  ]
}